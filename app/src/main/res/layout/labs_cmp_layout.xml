<?xml version="1.0" encoding="utf-8"?>

	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	    android:id="@+id/labs_cmp_layout"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical" >
	
	    <TextView
	        android:id="@+id/ast_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft = "true"
	        android:layout_alignParentTop = "true"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="0dp"
	        android:text="AST: "
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/alt_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="20dp"
	        android:text="ALT: "
	        android:textSize="8dp" />        
	    
	    <TextView
	        android:id="@+id/alkaline_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="40dp"
	        android:text="Alkaline Phosphatase: "
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/bilirubin_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="60dp"
	        android:text="Total Bilirubin: "
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/protein_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="80dp"
	        android:text="Protein: "
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/albumin_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="100dp"
	        android:text="Albumin:  "
	        android:textSize="8dp" />	    
	    
	    <TextView
	        android:id="@+id/ast"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="0dp"
	        android:text="0"
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/alt"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="20dp"
	        android:text="0"
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/alkaline_phosphatase"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="40dp"
	        android:text="0"
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/total_bilirubin"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="60dp"
	        android:text="0"
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/protein"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="80dp"
	        android:text="0"
	        android:textSize="8dp" />
	    
	    <TextView
	        android:id="@+id/albumin"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/ast_label"
	        android:layout_alignTop="@+id/ast_label"
	        android:layout_marginLeft="90dp"
	        android:layout_marginTop="100dp"
	        android:text="0"
	        android:textSize="8dp" />    
	
	    
	</RelativeLayout>
