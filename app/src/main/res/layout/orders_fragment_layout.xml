	        android:layout_height="wrap_content"
	        android:layout_alignTop="@+id/admit_to_label"
	        android:layout_toRightOf="@+id/admit_to_layout"
	        android:layout_marginLeft="10dp"
	        android:text="Diet: " />

	    
	    <LinearLayout
	        android:id="@+id/diet_layout"
        	android:layout_width="140dp"
        	android:layout_height="wrap_content"
        	android:layout_alignLeft="@+id/diet_label"
        	android:layout_below="@+id/diet_label"
        	android:orientation="vertical"
            android:background="@drawable/back">
	  
	         <CheckedTextView 
	            android:id="@+id/diet_regular_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="0dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Regular"/>
	        <CheckedTextView 
	            android:id="@+id/diet_cardiac_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="0dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Cardiac"/>
