			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="1dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "As tolerated"/>

	        <CheckedTextView
	            android:id="@+id/activity_level_up_to_chair_check"
