			    android:text = "Lovenox 40mg subcutaneous once daily"/>
	         
	        <CheckedTextView 
	            android:id="@+id/dvt_lovenox_twice_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="1dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Lovenox 40mg subcutaneous twice daily"/>
	        
	        <CheckedTextView 
	            android:id="@+id/dvt_heparin_once_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="1dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
