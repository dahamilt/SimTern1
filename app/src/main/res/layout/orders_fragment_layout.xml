			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Diabetic"/>
	        <CheckedTextView 
	            android:id="@+id/diet_cardiac_diabetic_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="0dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Cardiac/Diabetic"/>
	        <CheckedTextView 
	            android:id="@+id/diet_npo_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="0dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "NPO"/>     
	        <CheckedTextView 
	            android:id="@+id/diet_npo_after_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="0dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "NPO after midnight for procedure"/>     			    
	    </LinearLayout>	    
	    
	    <TextView
	        android:id="@+id/activity_level_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_below="@+id/diet_layout"
	        android:layout_alignLeft="@+id/admit_to_layout"
	        android:layout_marginTop="5dp"
	        android:text="Activity Level: " />

	    
	    <LinearLayout
	        android:id="@+id/activity_level_layout"
        	android:layout_width="140dp"
        	android:layout_height="wrap_content"
        	android:layout_alignLeft="@+id/activity_level_label"
        	android:layout_below="@+id/activity_level_label"
        	android:orientation="vertical"
            android:background="@drawable/back">
	  
	         <CheckedTextView 
	            android:id="@+id/activity_level_as_tolerated_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
