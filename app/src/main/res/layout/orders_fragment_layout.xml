        	android:layout_height="wrap_content"
        	android:layout_alignLeft="@+id/neuro_checks_label"
        	android:layout_below="@+id/neuro_checks_label"
        	android:orientation="vertical"
           android:background="@drawable/back">
	  
	         <CheckedTextView 
