    			android:focusable="true"
			    android:text = "Bed rest with bathroom privileges"/>
	        
	        <CheckedTextView 
	            android:id="@+id/activity_level_fall_precautions_check"
			    android:layout_width="match_parent"
			    android:layout_height="match_parent"
			    android:gravity="center_vertical"
			    android:checkMark="?android:attr/listChoiceIndicatorMultiple"
			    android:padding="1dp"
			    android:textSize="12sp"
			    android:clickable="true"
    			android:focusable="true"
			    android:text = "Fall precautions"/>     
			    
	    </LinearLayout>	    	    
	    
	    <TextView
	        android:id="@+id/vital_signs_label"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignTop="@+id/activity_level_label"
	        android:layout_toRightOf="@+id/activity_level_layout"
	        android:layout_marginLeft="10dp"
	        android:text="Vital Signs: " />

	   <LinearLayout
	        android:id="@+id/vital_signs_layout"
        	android:layout_width="140dp"
        	android:layout_height="wrap_content"
        	android:layout_alignLeft="@+id/vital_signs_label"
        	android:layout_below="@+id/vital_signs_label"
        	android:orientation="vertical"
           android:background="@drawable/back">
	  
	         <CheckedTextView 
	            android:id="@+id/vital_signs_per_floor_check"
			    android:layout_width="match_parent"
